config wifi-device 'radio0'
        option type 'mac80211'
        option macaddr 'a0:f3:c1:49:2a:e7'
        list ht_capab 'LDPC'
        list ht_capab 'SHORT-GI-20'
        list ht_capab 'SHORT-GI-40'
        list ht_capab 'TX-STBC'
        list ht_capab 'RX-STBC1'
        list ht_capab 'DSSS_CCK-40'
        option channel '1'
        option country 'DE'
        option txpower '20'
        option hwmode '11ng'
        option htmode 'HT40+'

config wifi-device 'radio1'        
        option type 'mac80211'     
        option macaddr 'a0:f3:c1:49:2a:e8'
        list ht_capab 'LDPC'              
        list ht_capab 'SHORT-GI-20'       
        list ht_capab 'SHORT-GI-40'       
        list ht_capab 'TX-STBC'           
        list ht_capab 'RX-STBC1'          
        list ht_capab 'DSSS_CCK-40'       
        option channel '44'               
        option country 'DE'               
        option txpower '17'               
        option hwmode '11na'       
        option htmode 'HT40+'      

config wifi-iface 'wifi_freifunk'         
        option device 'radio0'            
        option network 'freifunk'         
        option mode 'ap'                  
        option ssid 'hamburg.freifunk.net' 
        option encryption 'none'          
                                          
config wifi-iface 'wifi_mesh'             
        option device 'radio0'            
        option network 'mesh'             
        option mode 'adhoc'               
        option ssid 'f8:d1:11:87:52:2e'   
        option bssid 'f8:d1:11:87:52:2e'  
        option mcast_rate '12000' 
        
config wifi-iface 'wifi_freifunk5'        
        option device 'radio1'            
        option network 'freifunk'         
        option mode 'ap'                  
        option ssid 'hamburg.freifunk.net' 
        option encryption 'none'          
                                          
config wifi-iface                         
        option device 'radio0'            
        option mode 'ap'                  
        option ssid 'PrivatSSID'           
        option encryption 'psk-mixed'     
        option key 'PrivatKey'
        option network 'privat' 
        
config wifi-iface                                                                   
        option device 'radio1'                                                      
        option mode 'ap'                                                            
        option ssid 'PrivatSSID'                                                     
        option encryption 'psk2'                                                    
        option key 'PrivatKey'
        option network 'privat'                                                     
          
